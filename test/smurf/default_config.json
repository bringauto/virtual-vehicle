{
  "general-settings": {
    "period-ms": 1000
  },
  "logging": {
    "console": {
      "level": "DEBUG",
      "use": true
    },
    "file": {
      "level": "DEBUG",
      "use": false,
      "path": "../_logs/"
    }
  },
  "vehicle-settings": {
    "vehicle-provider-type": "simulation",
    "provider-simulation-settings": {
      "speed-override": true,
