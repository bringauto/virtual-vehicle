  },
  "fleet-settings": {
    "fleet-provider-type": "internal-protocol",
    "provider-internal-protocol-settings": {
      "module-gateway-ip":"172.17.0.1",
      "module-gateway-port": 1636,
      "device-role": "autonomy",
      "device-priority": 1,
      "reconnect-period-s": 20,
      "device-name": "smurf_virtual_vehicle"
    },
    "empty":{
    }
  }
}
