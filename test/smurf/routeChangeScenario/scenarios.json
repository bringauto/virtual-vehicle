{
  "setup" : [
    "docker compose --file=../etna/docker-compose.yml --env-file=.env --profile=without-devices up -d",
    "docker compose --file=../etna/docker-compose.yml --env-file=.env --profile=without-devices up -d",
    "sleep 5"
  ],
  "between_runs" : [
    "docker compose --file=../etna/docker-compose.yml --env-file=.env --profile=without-devices restart",
    "sleep 5"
  ],
  "scenarios" : [
    {
      "name" : "delayedChange",
      "timeout_s" : 100,
      "arguments" : [
        "--map", "../maps/route_change.osm",
        "--wait-at-stop-s", "10",
        "--speed-override", "30",
        "--config", "../default_config.json"
      ]
    }
  ],
  "cleanup" : [
    "docker compose --profile=without-devices --file=../etna/docker-compose.yml down"
  ]
}
